{
  "Framework": "jasmine",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "RootReferencePathMode": "SettingsFileDirectory",
  "EnableTracing": true,
  "References": [
    {
      "Includes": [

        "*/Scripts/*.ts",
        "*/typings/main.d.ts"

      ],
      "Excludes": [ "*/Scripts/*.d.ts" ]
    }
  ],
  "Tests": [
    { "Path": "scriptsTest" },
    {

      "Includes": [ "scriptsTest/*.ts" ]
    }
  ]
}